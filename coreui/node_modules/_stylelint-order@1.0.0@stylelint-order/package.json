{
  "name": "stylelint-order",
  "version": "1.0.0",
  "description": "A collection of order related linting rules for stylelint.",
  "keywords": [
    "stylelint-plugin",
    "stylelint",
    "css",
    "lint",
    "order"
  ],
  "author": "Aleks Hudochenkov <aleks@hudochenkov.com>",
  "license": "MIT",
  "repository": "hudochenkov/stylelint-order",
  "bugs": {
    "url": "https://github.com/hudochenkov/stylelint-order/issues"
  },
  "homepage": "https://github.com/hudochenkov/stylelint-order",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "rules",
    "utils",
    "!**/tests",
    "index.js"
  ],
  "main": "index.js",
  "dependencies": {
    "lodash": "^4.17.10",
    "postcss": "^7.0.2",
    "postcss-sorting": "^4.0.0"
  },
  "peerDependencies": {
    "stylelint": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "~5.3.0",
    "eslint-config-hudochenkov": "~3.0.0",
    "eslint-config-prettier": "~2.9.0",
    "eslint-plugin-prettier": "~2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "prettier": "~1.14.2",
    "stylelint": "^9.4.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "pretest": "eslint .",
    "test": "jest",
    "jest": "jest",
    "watch": "jest --watch",
    "coverage": "jest --coverage",
    "fix": "npm run pretest -- --fix"
  },
  "lint-staged": {
    "*.js": [
      "npm run fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./jest-setup.js"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.js$|rules/.*/tests/.*\\.js$"
  },
  "_from": "stylelint-order@1.0.0",
  "_resolved": "http://registry.npm.taobao.org/stylelint-order/download/stylelint-order-1.0.0.tgz"
}